{
  "name": "cdk-recaptcha-authorizer",
  "version": "2.2.0",
  "description": "An AWS CDK construct library that provides a reCaptcha Authorizer for API Gateway REST APIs",
  "author": "Aaron Lucia",
  "license": "Apache-2.0",
  "homepage": "https://constructs.dev/packages/cdk-recaptcha-authorizer",
  "repository": {
    "type": "git",
    "url": "https://github.com/engineal/cdk-recaptcha-authorizer.git"
  },
  "keywords": [
    "cdk",
    "constructs",
    "aws",
    "apigateway",
    "lambda",
    "recaptcha"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "is-ci || husky install",
    "lint": "eslint .",
    "build": "jsii",
    "watch": "jsii --watch",
    "package": "jsii-pacmak",
    "release": "jsii-release",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/aws-lambda": "^8.10.111",
    "@types/jest": "^27.5.2",
    "@types/node": "18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.52.0",
    "aws-cdk-lib": "2.65.0",
    "constructs": "10.0.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "jsii": "^1.76.0",
    "jsii-pacmak": "^1.76.0",
    "jsii-release": "^0.2.610",
    "mocked-env": "^1.3.5",
    "semantic-release": "^20.1.1",
    "ts-jest": "^27.1.5",
    "typescript": "~4.9.5"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.65.0",
    "constructs": "^10.0.0"
  },
  "bundleDependencies": [
    "aws-sdk",
    "aws-xray-sdk-core",
    "axios"
  ],
  "dependencies": {
    "aws-sdk": "^2.1328.0",
    "aws-xray-sdk-core": "^3.4.1",
    "axios": "^0.27.2"
  },
  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "java": {
        "package": "com.engineal.cdk.recaptchaauthorizer",
        "maven": {
          "groupId": "com.engineal.cdk",
          "artifactId": "recaptcha-authorizer"
        }
      },
      "python": {
        "distName": "cdk-recaptcha-authorizer",
        "module": "cdk_recaptcha_authorizer"
      },
      "dotnet": {
        "namespace": "EngineAL.CDK.RecaptchaAuthorizer",
        "packageId": "EngineAL.CDK.RecaptchaAuthorizer"
      }
    }
  }
}
